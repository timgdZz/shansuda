.box {
  width: 100%;
  height: 850rpx;
  background: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.title {
  font-size: 38rpx;
  font-weight: 700;
}
.allInput {
  width:100%;
  padding-left: 10rpx;
}
.inp {
  width: 100%;
  height: 110rpx;
  border-bottom: 2rpx solid #f4f4f4;
  font-size: 30rpx;
}
.confirm {
  width: 100%;
  height: 85rpx;
  line-height: 85rpx;
  text-align: center;
  background-color: #dbdbdb;
  color: #fff;
  font-size: 28rpx;
  font-weight: 400;
}