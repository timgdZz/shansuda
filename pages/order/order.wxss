.title {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-around;
  padding: 0 50rpx;
  box-sizing: border-box;
  font-size: 24rpx;
}
.active {
  color: #fe9127;
  font-weight: 700;
  border-bottom:  2rpx solid #fe9127;
}
.all {
  width: 100%;
  height: 340rpx;
  background-color: #fff;
  margin-top: 20rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}
page {
  background-color: #f0f0f0;
}
.boxTop {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid #ccc;
  display: flex;
  justify-content: space-between;
  font-size:24rpx;
}
.boxImg {
  display: flex;

}
.boxImg image {
  margin-top: 25rpx;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.boxContent {
  width: 100%;
  height: 260rpx;
  padding-top: 20rpx;
  box-sizing: border-box;

}
.boxContent image {
  display: block;
  width: 180rpx;
  height: 120rpx;
}
.boxName {
  width: 100%;
  font-size: 24rpx;
  margin-left: 20rpx;
}
.boxContentTop {
  display: flex;
  margin-bottom: 20rpx;
}
.order {
  font-size: 24rpx;
}