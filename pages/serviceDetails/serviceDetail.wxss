.headImg {
  width: 100%;
  height: 630rpx;
}
.headImg swiper {
  height: 630rpx;
}
.headImg image {
  width: 100%;
  height:630rpx;
}
.wx-swiper-dots{position:relative;}
.wx-swiper-dots.wx-swiper-dots-horizontal{
margin-bottom: 40rpx;
}
.title {
  padding-left: 20rpx;
  padding-top: 24rpx;
  width: 100%;
  height: 130rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.title {
  font-size: 32rpx;
  font-weight: 700;
}
.title view:nth-of-type(2) {
  margin-top: 14rpx;
  font-size: 24rpx;
  color: #7d7d7d;
}
.shadow {
  width: 100%;
  height: 24rpx;
  background-color: #efefef;
}
.content,
.time {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.boxAll {
  width: 100%;
  /* height: 1334rpx; */
  z-index: -10;
}
.box {
  position: fixed;
  /* top: -1334rpx; */
  top: 0;
  width: 100%;
  height: 700rpx;
  background-color: rgba(0,0,0,.5);
  transition: all 1s;
}
.box1 {
  position: fixed;
  /* top: -1334rpx; */
  top: 0;
  width: 100%;
  height: 700rpx;
  background-color: rgba(0,0,0,0);
  transition: all 1s;
  z-index: -10;
}
.picker {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 800rpx;
  background-color: #fff;
  transition:  .5s;
}
.picker1 {
  position: fixed;
  bottom:0;
  width: 100%;
